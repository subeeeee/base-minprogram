
.container .van-search__content{
  border-radius: 100rpx;
  background: #f5f5f5;
}

.height20{
  height: 20rpx;
}

.gray{
  background: #f5f5f5;
}

.screen{
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  justify-content: space-around;
  box-shadow: 0 4rpx 8rpx 0 rgba(0,0,0,0.05);
}
.screen .li{
  max-width: 130rpx;
  padding-right: 36rpx;
  font-size: 32rpx;
  color: #4A4A4A;
  background: url("https://yxf1.juzhouyun.com/xcx_images/customer_arrow.png") no-repeat right center /14rpx 8rpx;

}
.screen .li text{
  display: block;
  overflow : hidden;
  white-space : nowrap;
  text-overflow : ellipsis;
}
.list .li .span{
  display: flex;
  align-items: center;
}
.list .i.icon.iconfont{
  width: 52rpx;
  height: 40rpx;
}



  
.list .i.icon.iconfont.icon-youxiao{
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-youxiao.png") no-repeat left center /52rpx 40rpx;
}


.list .i.icon.iconfont.icon-wuxiao{
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-wuxiao.png") no-repeat left center /52rpx 40rpx;
}
.list .i.icon.iconfont.icon-guoqi{
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-guoqi.png") no-repeat left center /52rpx 40rpx;
}
.list .i.icon.iconfont.icon-daofangyuqi{color:#af9564;
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-daofangyuqi.png") no-repeat left center /52rpx 40rpx;
}
.list .i.icon.iconfont.icon-daiqueren{color:#3d8ade;
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-daiqueren.png") no-repeat left center /52rpx 40rpx;
}
.list .i.icon.iconfont.icon-chengjiaoyuqi{color:#14386c;
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-chengjiaoyuqi.png") no-repeat left center /52rpx 40rpx;
}

.list .i.icon.iconfont.icon-yibaobei{
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-yibaobei.png") no-repeat left center /52rpx 40rpx;
}

.list .icon-erweima{
  /* margin-right: 80rpx; */
  width: 36rpx;
  height: 80rpx;
  background: url("https://yxf1.juzhouyun.com/xcx_images/icon-erweima.png") no-repeat left center /36rpx 36rpx;
}


  .list .ul{width:100%;height:162rpx;border-bottom:2rpx solid #f5f5f5;}
  .list .ul .li{height:80rpx;line-height:80rpx;padding:0 20rpx;display:flex;justify-content:space-between;position:relative;}
  .list .span .i.icon.iconfont{font-size:38rpx;margin-right:10rpx;}
  .list .span:last-child{white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
  .list .type0{color:#ffa947}
  .list .type1{color:#5cb9fe}
  .list .type2{color:#ff6666}
  .list .type3{color:#58c06f}
  .list .type4{color:#00c100}
  .list .type5{color:#fc1e43}
  .list .type6{color:#dddddd}
  .list .ul .li:last-child{font-size:26rpx;color:#999}





  .popup{
    height: 100%;
  }
  .popup .ul{
    /* max-height: 400rpx; */
    margin:0 38rpx;
    overflow-y: auto;
  }
  .popup .ul .li{
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    color: #4A4A4A;
    border-bottom:1rpx solid #f5f5f5;
    text-align: left;
  }
  .popup .ul .li.active{
    color:var(--dark-color)
  }

  .sortType{height:60rpx;line-height:60rpx;font-size:28rpx;font-weight:bold; text-align:right;padding-right:20rpx;color:#666;}

  .QrcodePop{
     height: 100%;
  }
  .close-btn{
    margin-top: 20rpx;
    text-align: right;
  }

  .canvas-code{
    margin-top: 100rpx;
  }

  .width160{
