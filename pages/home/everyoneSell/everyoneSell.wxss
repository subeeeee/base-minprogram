@import '/assets/css/common.wxss';


.form_box{
  height: calc(100% - 500rpx);
  overflow: auto;
}
.form-btn-warp{
  border-top: 1px solid #F5F5F5;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.form-btn{
  width: 80%;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: var(--dark-color);
  border-radius: 30rpx;
  font-size: 24rpx;
  color: #fff;
}