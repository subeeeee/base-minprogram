@import '/assets/css/common.wxss';

/* 页面专属样式 */

.container {
  /* padding:0 var(--home-index-padding); */
  background:var(--home-background-color);
  box-sizing: border-box;
}
.icon-right-single{
  color:var(--dark-color);
  padding-left:10rpx;
}

/* 顶部搜索条 */

.top-bar-box {
  /* padding:0 var(--home-index-padding);  
   width: 100%;
  */
  width: var(--home-index-width);
  margin: 0 auto;
  height: 380rpx;
  /* background: linear-gradient(180deg, rgba(21, 134, 238, 1) 0%, rgba(10, 172, 227, 1) 39%, rgba(1, 202, 218, 1) 100%); 
  border-radius: 0 0 50% 50%;*/
  padding-top: 10rpx;
  box-sizing: border-box;
}

.top-bar {
  width: 100%;
  height: 70rpx;
  margin: 0 auto;
  background: #f5f5f5;
  box-sizing: border-box;
  border-radius: 8rpx;
  overflow: hidden;
}

.top-bar-box .city-wrap {
  float: left;
  margin-left: 32rpx;
  margin-top: 20rpx;
  height: 30rpx;
  font-size: 28rpx;
  line-height: 30rpx;
  padding-right: 22rpx;
  border-right: 2rpx solid #d8d8d8;
  color: #4a4a4a;
}
.top-bar-box .city-input{
  float: left;
}
.top-bar-box .query-icon {
  float: left;
  margin-left: 18rpx;
  margin-top: 22rpx;
  width: 28rpx;
  height: 28rpx;
}

.top-bar-box .city-wrap image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 22rpx;
}

.top-bar-box .input-wrap {
  float: left;
  line-height: 40rpx;
  height: 40rpx;
  margin-left: 24rpx;
  margin-top: 16rpx;
  font-size: 28rpx;
}

.place-holder {
  color: #bdbdbd;
}

/* banner */

.activity {
  width: 100%;
  height: 340rpx;
  position: absolute;
  top: 100rpx;
  left: 50%;
  transform: translate(-50%, 0);
  border-radius: 20rpx;
  overflow: hidden;
  box-sizing: border-box;
  /* padding:0 var(--home-index-padding); */
  width: var(--home-index-width);
  margin: 0 auto;
  
}
.activity image, .activity video{
  width: 100%;
  height: 100%;
}

/*  轮播 */

.imageContainer {
  width: 100%;
  height: 300rpx;
  border: 2rpx solid #eee;
  border-radius: 8rpx;
}

.itemImg {
  width: 100%;
  height: 220rpx;
  border-radius: 8rpx 8rpx 0 0;
}
.itemName{
  font-size:28rpx;
  color: #303030;
  line-height:48rpx;
  text-indent: 20rpx;
  overflow: hidden;
  height: 48rpx;
}
.itemTime{
  font-size:24rpx;
  color: #9B9B9B;
  text-indent: 20rpx;
}
.headerModleSwiper{
    /* height:150rpx; */
    height:120rpx;
    margin-top: 20rpx;
}
.modelDots{
  width:60rpx;
  height:6rpx;
  background: var(--dark-color);
  margin:0 auto;
  display: flex;
}
.mdDot{
  width:30rpx;
  height:6rpx;
  flex:1;
}
.mdDot.active{
  background: #D0D0D0;
}
.headerModle {
  width: 100%;
  overflow: hidden;
  margin-top: 104rpx;
  margin-bottom: 16rpx;
  display: flex;
  justify-content: space-around;
}
.headerModle image{
  width: 100%;
  height: 100%;
}
.headerModle .item {
  text-align: center;
}

.headerModle .item .img {
  width: 48rpx;
  height: 42rpx;
  margin: 0 auto 14rpx;
  /* border-radius: 50%; */
}
.headerModle .item:nth-child(2) .img {
  width: 44rpx;
  height: 46rpx;
  margin-bottom:10rpx;
}
.headerModle .item:nth-child(3) .img {
  width: 50rpx;
  height: 48rpx;
  margin-bottom:8rpx;
}
.headerModle .item:nth-child(4) .img {
  width: 44rpx;
  height: 44rpx;
  margin-bottom:12rpx;
}
.headerModle .item:nth-child(5) .img {
  width: 46rpx;
  height: 46rpx;
  margin-bottom:16rpx;
}

.headerModle .item text {
  color: #4a4a4a;
  font-size: 24rpx;
  font-weight: 500;
  line-height: 30rpx;
  font-weight:bold;
}

