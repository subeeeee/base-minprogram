.houseDetailBox{
	position:fixed;
	top:0;
	left:0;
	background: rgba(0,0,0,0.3);
	width: 100%;
	height: 100%;
}
.houseDetail{
	position:absolute;height:486rpx;bottom:0;left:0;background:#ffffff;padding:30rpx 0 50rpx;width:100%;
}
.houseDetail .project{margin:0 30rpx;}
.closeDetail{
	width: 60rpx;
height: 60rpx;
border: 1px solid #ccc;
font-size: 24rpx;
position: absolute;
right: 0;
top: -25rpx;
border-radius: 50rpx;
line-height: 60rpx;
text-align: center;
background: #ffffff;
z-index: 100;
}

