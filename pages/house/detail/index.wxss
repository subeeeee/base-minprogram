}
/* box */

.house-box .box-ctrl {
  padding : 0 30rpx 20rpx;
}
/* 置业顾问 */

.consultant-wrap {
  padding-bottom : 20rpx;
}
.consultant-item {
  padding : 30rpx;
}
.consultant-ctrl {
  font-size : 26rpx;
  line-height : 72rpx;
  display : flex;
  justify-content : center;
  height : 72rpx;
  color : #999;
}
.consultant-ctrl .icon-refresh {
  font-size : 32rpx;
  margin-right : 10rpx;
}

/* map */
.map-wrap .map {
  margin-top : 10rpx;
}
.map-wrap .map map {
  width : 100%;
  height : 335rpx;
}
.map-tab-hd {
  display : flex;
  padding : 20rpx 45rpx;
}
.map-tab-control-item {
  position : relative;
  flex : 1;
  text-align : center;
}
.map-tab-control-item .arrow {
  display : none;
}
.map-tab-control-item .icon {
  line-height : 72rpx;
  width : 72rpx;
  height : 72rpx;
  margin : 0 auto;
  text-align : center;
  color : #ccc;
  border : 2rpx solid #ccc;
  border-radius : 50%;
}
.map-tab-control-item .icon [class*='icon-'] {
  font-size : 42rpx;
}
.map-tab-control-item .text {
  font-size : 24rpx;
  margin-top : 10rpx;
  color : #666;
}
.map-tab-control-item.active .icon {
  color : #f7650f;
  border-color : #f7650f;
}
.map-tab-control-item.active .text {
  color : #f7650f;
}
.map-tab-control-item.active .arrow {
  font-size : 0;
  line-height : 0;
  position : absolute;
  bottom : -20rpx;
  left : 50%;
  display : block;
  transform : translateX(-50%);
  border-width : 20rpx;
  border-style : dashed;
  border-color : #fafafa;
  border-top-width : 0;
  border-right-color : transparent;
  border-bottom-style : solid;
  border-left-color : transparent;
}
.map-tab-bd {
  padding : 0 30rpx 40rpx;
}
.map-tab-content {
  position : relative;
  padding : 20rpx;
  background-color : #fafafa;
}
.map-tab-content .content-item {
  font-size : 24rpx;
  line-height : 2;
  color : #333;
}
/* 猜我喜欢 */

.like-list {
  padding-bottom : 10rpx;
}
.like-item {
  padding : 20rpx 30rpx;
}
.like-item + .like-item {
  border-top : 2rpx solid #eee;
}
.adviserItem {
  display: flex;
  height: 60rpx;
