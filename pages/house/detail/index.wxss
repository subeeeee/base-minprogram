@import '/assets/css/common.wxss';
@import '/assets/css/houseDetail.wxss';
.icon-right-single{
    font-size: 28rpx;
    color:var(--dark-color);
}
.detailsTopBlock {
  width: 100%;
  position: relative;
}
.overView, .swiper {
  width: 100%;
  height: 400rpx;
}

.overView > view {
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 1;
  left: 0;
  top: 0;
}
.overView > view image{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.videoBox {
  width: 100%;
  height: 400rpx;
}
.videoBox > video {
  width: 100%;
}

.swiperLabel {
  width: 100%;
  position: absolute;
  z-index: 2;
  bottom: 20rpx;
  left: 0;
  text-align: center;
}
.swiperLabel > text {
  display: inline-block;
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin: 0 16rpx;
  text-align: center;
  background: #000;
  border-radius: 20rpx;
  font-size: 24rpx;
  color: #fff;
}
.swiperLabel > text.selectItem {
   background: var(--dark-color);
 }

image {
  display: block;
  width: 100%;
  height: 100%;
}
.container {
  position : relative;
  box-sizing : border-box;
  height : 100vh;
  background-color : #f6f6f6;
}
/* 基本信息 */
.detailsBlock {
  padding: 0 40rpx 26rpx;
  background: #FFFFFF;
}
.address {
  font-size: 32rpx;
  padding-bottom: 14rpx;
  position: relative;
}
.weizhi{
  width: 24rpx;
  height: 32rpx;
  position: absolute;
  right: 0;
  top: 12rpx;
}
.detailsBlock>.title {
  display: flex;
  height: 100rpx;
}
.detailsBlock>.title > text {
  flex: 1;
  height: 40rpx;
  line-height: 40rpx;
  padding-top: 30rpx;
}
.detailsBlock>.title > text:first-child {
   font-size: 32rpx;
   color: #4A4A4A;
 }

.detailsBlock>.title > text:last-child {
   text-align: right;
   font-size: 32rpx;
   color: var(--dark-color)
 }

.detailsTitle {
  position: relative;
  background: #fff;
  margin-bottom: 20rpx;
}
.detailsTitle > .call{
  height: 120rpx;
  margin: 0 auto 20rpx auto;
  background: #fff7fa;
  padding: 16rpx 0 0 28rpx;
  color: var(--dark-color);
  position: relative;
  border-radius: 12rpx;
  box-sizing: border-box;
}
.detailsTitle > .call > .phone{
  font-size: 40rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-weight: 500;
}
.detailsTitle > .call > .desc{
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-weight: 400;
}
.detailsTitle > .call > .image{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  background:#fff;
  border-radius: 50%;
  text-align:center;

}
.detailsTitle > .call > .image image{
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
}
.detailsTitle > .subscription{
  background: url(https://yxf-bucket-01.oss-cn-beijing.aliyuncs.com/Pattern/1587894141222.jpg) no-repeat;
  background-size: 100% 100%;
}
.detailsTitle > .seeRoom{
  background: url(https://yxf-bucket-01.oss-cn-beijing.aliyuncs.com/Pattern/1587894197880.jpg) no-repeat;
  background-size: 100% 100%;
}
.detailsTitle > .subscription, .detailsTitle > .seeRoom{
  height: 120rpx;
  border-radius: 12rpx;
  color: #4A4A4A;
  padding: 18rpx 0 0 28rpx;
  box-sizing: border-box;
  position: relative;
  margin: 0 auto 20rpx auto;
}
.subscription > .title, .seeRoom > .title {
  font-size: 36rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-weight: 500;
}
.subscription > .desc, .seeRoom > .desc {
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-weight: 400;
}
.subscription > .btn, .seeRoom > .btn {
  width: 180rpx;
  height: 56rpx;
  color: #F5A623;
  font-size: 28rpx;
  text-align: center;
  line-height: 56rpx;
  border-radius: 28rpx;
  border: 2rpx solid #F5A623;
  background: #fff;
  position: absolute;
  top: 32rpx;
  right: 20rpx;
}
.detailsTitle>.titleText {
  display: block;
  font-size: 40rpx;
  color: #4A4A4A;
  padding: 30rpx 0 16rpx 0;
