  white-space: nowrap;
  text-overflow: ellipsis;
}

.detailsTitle>.detail>.goUrl {
  font-size: 24rpx;
  color: #4A4A4A;
  position: absolute;
  right: 0;
  top: 0;
}
.detailsTitle>.detail .icon-right-single{
  font-size: 24rpx;
}
.onlookers{
  overflow: hidden;
  margin-top:8rpx;
  margin-bottom: 20rpx;
}
.onlookers .text{
  float: left;
  margin-left: 54rpx;
  font-size: 28rpx;
