  margin-right: 20rpx;
}
.adviserItem > .cz {
  text-align: right;
  font-size: 60rpx;
  color: var(--dark-color);
}
/* 资讯 */
.topic{
  padding: 32rpx 44rpx 0 44rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.topic .top{
  overflow: hidden;
  height: 56rpx;
  margin-bottom: 24rpx;
  color: #4A4A4A;
}
.topic .title{
  float: left;
  font-size: 40rpx;
  line-height: 56rpx;
}
.topic .goAll{
  float: right;
  font-size: 24rpx;

}
.topic-item{
  overflow: hidden;
  padding-bottom: 28rpx;
  border-bottom: 2rpx solid #F5F5F5;
  padding-top: 30rpx;
}
.topic-item:last-child{
  border: 0;
}
.topic-item > .left{
  float: left;
  width: 396rpx;
  margin-right: 24rpx;
}
.topic-item > .left > .title{
  color: #4A4A4A;
  font-size: 32rpx;
  height: 44rpx;
  line-height: 44rpx;
  margin-bottom: 20rpx;
  white-space: nowrap;
  text-overflow:ellipsis;
  width: 100%;
  overflow: hidden;
  font-weight: 500;
}
.topic-item > .left > .desc{
  color: #4A4A4A;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-bottom: 2rpx;
  line-clamp: 2;
  box-orient: vertical;
  overflow: hidden;
  width: 100%;
  min-height: 80rpx;
  font-weight: 400;
}
.topic-item > .left > .date{
  color: #9B9B9B;
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  width: 100%;
}
.topic-item > .right{
  width: 240rpx;
  height: 180rpx;
  float: left;
}
.topic-item > .right > image{
  border-radius: 12rpx;
}
