.container {
  box-sizing : border-box;
  height : 100vh;
  background-color : #fff;
  margin-bottom: 160rpx;
}
.house-box{
  padding: 32rpx 22rpx 0 22rpx;
}
.box-hd{
  height: 50rpx;
  line-height: 50rpx;
  margin-bottom: 28rpx;
}
.image-list {
  display : flex;
  flex-wrap : wrap;
}
.image-item {
  box-sizing : border-box;
  width: 160rpx;
  height: 120rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.image-item:nth-child(4n+0){
  margin-right: 0;
}
.image-item image {
  width : 100%;
  height : 100%;
}
.btns{
  width: 100%;
  height: 160rpx;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  background: #fff;
  padding-top: 40rpx;
  box-sizing: border-box;
  border-top: 2rpx solid #bbb;
}
.btns view{
  width: 280rpx;
  height: 80rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
}
.btns view:first-child{
  margin-right: 40rpx;
  margin-left: 76rpx;
      /* background:linear-gradient(270deg,rgba(255,146,80,1) 0%,rgba(252,99,57,1) 100%); */
    background:var(--dark-color);
}
.btns view:last-child {
  /* background: linear-gradient(90deg, rgba(26, 135, 241, 1) 0%, rgba(49, 181, 249, 1) 100%); */
  background:var(--dark-color);
}

.mingYueJiangNan-btn-color{
  background-color: #0ED4A7 !important;
}
