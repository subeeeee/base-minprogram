.blue{
    color:var(--dark-color) !important;
}
.iconR{
    color:var(--dark-color) ;
    float: right;
    display: inline-block;
}
.detailsBlock {
    padding: 20rpx 26rpx;
    border-bottom: 20rpx solid #f4f4f4;
    background: #FFFFFF;
}
.detailsBlock > .title {
    display: flex;
    height: 100rpx;
}
.detailsBlock > .title > text {
    flex: 1;
    height: 40rpx;
    line-height: 40rpx;
    padding-top: 30rpx;
}
.detailsBlock > .title > text:first-child {
     font-size: 32rpx;
     color: #4A4A4A;
 }
.detailsBlock > .title > text:last-child {
     text-align: right;
     font-size: 24rpx;
     color: #9B9B9B
 }

.detailsTitle {
    position: relative;
    background: #fff;
}
.detailsTitle>.detail{
    font-size: 24rpx;
    line-height: 60rpx;
}
.detailsTitle>.detail>text{
    font-size: 24rpx;
    color: #9B9B9B;
    display:inline-block;
}
.detailsTitle>.detail>.control{
    font-size: 24rpx;
    color: #4A4A4A;
    display:inline-block;
}
.detailsTitle>.detail>.goUrl{
    font-size: 20rpx;
    color: #9B9B9B;
    float: right;
}
.detailsTitle > .titleText {
    display: block;
    font-size: 32rpx;
    color: #4A4A4A;
    padding: 30rpx 0 16rpx 0;
}
.detailsTitle > .money{
    font-size: 32rpx;
    color: #D12945;
    line-height: 60rpx;
}
.detailsTitle > .label {
    font-size: 24rpx;
    line-height: 32rpx;
    overflow: hidden;
    margin: 10rpx 0;
}
.detailsTitle > .label > text {
    height: 32rpx;
    background: #F6F6F6;
    color: #9B9B9B;
    /* padding: 0 20rpx;
    margin-right: 20rpx; */
    padding: 2rpx 8rpx;
    margin-right: 8rpx;
    margin-bottom: 8rpx;
    display: inline-block;
    border-radius: 4rpx;
}



.detailsTitle>.label>text.onSale {
    margin-left: 0;
    background: var(--dark-color);
    color: #fff;
  }
 
 .detailsTitle>.label>text.forSale {
    margin-left: 0;
    background: #ffac24;
    color: #fff;
  }
 
 .detailsTitle>.label>text.forSaled {
    margin-left: 0;
    background: #f94641;
    color: #fff;
  }
 
 .detailsTitle>.label>text.forNoSaled {
    margin-left: 0;
    background: #9b9b9b;
    color: #fff;
  }

.detailsTitle > .address {
    font-size: 24rpx;
    color: #4A4A4A;
    position: relative;
}
.detailsTitle > .detailsMore {
    position: absolute;
    right: 26rpx;
    top: 32rpx;
    width: 110rpx;
    height: 32rpx;
    line-height: 32rpx;
    color: var(--dark-color);
    font-size: 24px;
    border: 2rpx solid var(--dark-color);
    text-align: center;
    border-radius: 16rpx;
}











.apartmentLayout > .cont {
    display: flex;
    flex-wrap: wrap;
}
.apartmentLayout > .cont > apartment {
    width: 50%;
    box-sizing: border-box;
}
.apartmentLayout > .cont > apartment:nth-child(2n-1) {
     padding-right: 16rpx;
 }

.apartmentLayout > .cont > apartment:nth-child(2n) {
     padding-left: 16rpx;
 }

.ad {
    display: flex;
    padding-top: 26rpx;
}
.ad > .adItem {
    width: 48%;
    background: #fff7fa;
    border-radius: 12rpx;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    font-size: 24rpx;
    color: var(--dark-color);
    box-sizing: border-box;
}
.ad > .adItem:nth-child(2n-1) {
     padding-right: 0;
     padding-left: 0;
 }

.ad > .adItem:nth-child(2n) {
     margin-left: 4%;
 }

.ad > .adItem > image {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}

.cont > .form_item:last-child{
    border: none;
}
.form_item {
    min-height: 100rpx;
    font-size: 32rpx;
    width: 100%;
    line-height: 100rpx;
    border-bottom: 2rpx solid #F5F5F5;
    box-sizing: border-box;

}
.form_item > .t {
    float: left;
    width: 200rpx;
    text-align: right;
    font-size:28rpx;
    color: #9b9b9b;
}

.form_item > .control {
    margin-left: 200rpx;
    color:#4a4a4a;
    font-size: 28rpx;
    margin-right: 25rpx;
}
.form_item > .control > input {
    height: 60rpx;
    line-height: 60rpx;
    background: #F6F6F6;
    padding: 0 20rpx;
}
.btns{
    width: 100%;
    height: 160rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    border-top: 2rpx solid #bbb;
    display: flex;
    box-sizing: border-box;
    padding: 40rpx 0 0 76rpx;
    background: #fff;
}
.btns view{
    width: 280rpx;
    height: 80rpx;
    border-radius: 10rpx;
    color: #fff;
    text-align: center;
    line-height: 80rpx;
    font-size: 28rpx;
}
.btns view:first-child{
    background:var(--dark-color);
}
.btns view:last-child{
    margin-left: 40rpx;
    background:var(--dark-color);
}

.weizhi{
    width: 24rpx;
    height: 32rpx;
    position: absolute;
    right: 0;
    top: 35rpx;
}

.mingYueJiangNan-btn-color{
    background-color: #0ED4A7 !important;
}
