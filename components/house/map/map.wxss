


.myMap {
  width: 100%;
  height: 200px;
}
page,.container-big{
  height: 100%;
}
.container-big .myMap{
  height:100%;
}


.areaList > .areaItem {
  height: 30px;
  line-height: 30px;
  font-size: 12px;
}
.areaList > .areaItem>.t {
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  margin-right: 90px;
}

.areaList > .areaItem>.m {
  display: block;
  width: 80px;
  float: right;
  color: #999;
  text-align: right;
}

.search_list_icon {
  display: flex;
  padding: 10px 0;
  font-size: 12px;
  color: #4A4A4A;
}
.search_list_icon > .search_item {
  width: 16.66%;
  text-align: center;
  height: 102rpx;
  position: relative;
}
.search_list_icon > .search_item > .search_text{
 width: 100%;
 text-align: center;
 position: absolute;
 left: 0;
 bottom: 0;
}
.search_list_icon > .search_item > .search_image {
  display: block;
  margin: 0 auto 6px auto;
}
.search_list_icon > .search_item1 .search_image{
  width: 22px;
  height: 28px;
}
.search_list_icon > .search_item2 .search_image{
  width: 27px;
  height: 25px;
}
.search_list_icon > .search_item3 .search_image{
  width: 24px;
  height: 24px;
}
.search_list_icon > .search_item4 .search_image{
  width: 24px;
  height: 24px;
}

.search_list_icon > .search_item5 .search_image{
  width: 22px;
  height: 25px;
}
.search_list_icon > .search_item6 .search_image{
  width: 20px;
  height: 23px;
}
.active{
  color: var(--dark-color);
  font-weight: bold;
}


.fixed{
  position: fixed;
  bottom:0;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.fixed .areaList{
  height: 250rpx;
  overflow-y: auto;
  background: #fff;
}
